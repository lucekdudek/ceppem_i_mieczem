<?xml version="1.0" encoding="utf-8"?>
<location>
  <background>podroz.jpg</background>
  <console>
    {console}
  </console>
  <!-- conversation with farmer -->
  <event place="{EVENTA}">
    <text>{conversation with} {governor}</text>
    <onclick>PERSON_CONVERSATION_GOVERNOR</onclick>
  </event>
  <!-- conversation with farmer's wife -->
  <event place="{EVENTB}">
    <text>{conversation with} {blacksmith}</text>
    <onclick>PERSON_CONVERSATION_BLACKSMITH</onclick>
  </event>
  <!-- attack farmer -->
  <event place="{EVENTC}">
    <text>{visit} {tavern}</text>
    <onclick>VISIT_TAVERN</onclick>
  </event>
  <!-- open chest -->
  <event place="{EVENTD}">
    <text>{visit} {bordelo}</text>
    <onclick>VISIT_BORDELO</onclick>
  </event>
  <road>
    <place onclick="GOTO_SMALL_FARM">{small_farm}</place>
  </road>
</location>