<?xml version="1.0" encoding="UTF-8"?>
<view>
	<!-- background -->
	<element>
		<x>0</x>
		<y>0</y>
		<width>1280</width>
		<height>720</height>
		<textures>
			<texture>background.png</texture>
			<texture>image2.png</texture>
		</textures>
	</element>
	<!-- exit -->
	<element>
	<x>100</x>
	<y>100</y>
	<width>100</width>
	<height>100</height>
	<onclick>exit</onclick>
	<onhover>
		<texture>image_with_alpha.png</texture>
	</onhover>
	<textures>
		<texture>image1.png</texture>
		<texture>image2.png</texture>
		<text>{exit}</text>
	</textures>
	</element>
</view>
